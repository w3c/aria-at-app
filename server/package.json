{
    "name": "server",
    "version": "1.0.0",
    "description": "Server for ARIA-AT App",
    "main": "index.js",
    "scripts": {
        "dev": "DOTENV_CONFIG_PATH=../config/dev.env nodemon --ignore './scripts/' --ignore './resources/' -r dotenv/config server.js --watch",
        "dev-debug": "DEBUG=express:* DOTENV_CONFIG_PATH=../config/dev.env nodemon --ignore './scripts/' --ignore './resources/' --inspect -r dotenv/config server.js --watch",
        "jest": "SEQUELIZE_TEST_HELPERS_JEST_RUNNER=true dotenv -e ../config/test.env -- jest",
        "prettier": "prettier --write \"**/*.{js,jsx,css}\"",
        "lint": "eslint --ext js,jsx .",
        "db-import-tests:dev": "DOTENV_CONFIG_PATH=../config/dev.env node -r dotenv/config ./scripts/import-tests/index.js",
        "db-import-tests:test": "DOTENV_CONFIG_PATH=../config/test.env node -r dotenv/config ./scripts/import-tests/index.js",
        "db-populate-sample-data:dev": "DOTENV_CONFIG_PATH=../config/dev.env node -r dotenv/config scripts/populate-test-data/index.js",
        "db-populate-sample-data:test": "DOTENV_CONFIG_PATH=../config/test.env node -r dotenv/config scripts/populate-test-data/index.js"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/bocoup/aria-at-app.git"
    },
    "license": "SEE LICENSE IN LICENSE.md",
    "bugs": {
        "url": "https://github.com/bocoup/aria-at-app/issues"
    },
    "homepage": "https://github.com/bocoup/aria-at-app#readme",
    "dependencies": {
        "@moebius/http-graceful-shutdown": "^1.1.0",
        "apicache": "^1.6.3",
        "apollo-server": "^3.13.0",
        "apollo-server-core": "^3.13.0",
        "apollo-server-express": "^3.13.0",
        "axios": "^0.27.2",
        "body-parser": "^1.20.2",
        "connect-history-api-fallback": "^2.0.0",
        "connect-pg-simple": "^9.0.1",
        "cross-spawn": "^7.0.3",
        "dotenv": "^16.4.5",
        "dotenv-cli": "^7.4.2",
        "express": "^5.0.0",
        "express-handlebars": "^7.1.3",
        "express-session": "^1.18.0",
        "fs-extra": "^11.2.0",
        "graphql": "^16.9.0",
        "js-base64": "^3.7.7",
        "jsonwebtoken": "^9.0.2",
        "lodash": "^4.17.21",
        "minimist": "^1.2.8",
        "moment": "^2.30.1",
        "node-cache": "^5.1.2",
        "nodemon": "^2.0.22",
        "object-hash": "^3.0.0",
        "pg": "^8.12.0",
        "pg-hstore": "^2.3.4",
        "rawgit": "https://github.com/rgrove/rawgit.git#eaa9b36aa2143041894866ba9db72e51d05c2a52",
        "sequelize": "^6.37.3",
        "shared": "1.0.0",
        "supertest-session": "^4.1.0",
        "uuid": "^10.0.0"
    },
    "devDependencies": {
        "@testing-library/dom": "^10.3.1",
        "eslint": "^8.57.0",
        "eslint-config-prettier": "^8.10.0",
        "eslint-plugin-jest": "^27.9.0",
        "eslint-plugin-json": "^3.1.0",
        "eslint-plugin-prettier": "^4.2.1",
        "jest": "^29.7.0",
        "jsdom": "^24.1.0",
        "jsdom-global": "^3.0.2",
        "node-mocks-http": "^1.15.0",
        "prettier": "^2.8.8",
        "sequelize-cli": "^6.6.2",
        "sequelize-test-helpers": "1.3.3",
        "supertest": "^6.3.4"
    }
}
